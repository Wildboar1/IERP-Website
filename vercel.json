{
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "buildEnv": [
    "ADMIN_DISCORD_IDS"
  ],
  "env": {
    "FORCE_REBUILD": "true"
  }
}
